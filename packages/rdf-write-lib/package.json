{
  "name": "@telicent-oss/rdf-write-lib",
  "version": "1.0.2",
  "private": false,
  "description": "Allows writing of RDF data in Telicent CORE platform",
  "main": "./dist/rdf-write-lib.cjs.js",
  "files": [
    "dist/*"
  ],
  "type": "module",
  "module": "./dist/rdf-write-lib.es.js",
  "types": "./dist/rdf-write-lib.d.ts",
  "exports": {
    ".": {
      "import": "./dist/rdf-write-lib.es.js",
      "require": "./dist/rdf-write-lib.cjs.js",
      "types": "./dist/rdf-write-lib.d.ts"
    }
  },
  "scripts": {
    "start": "vite",
    "test": "jest --runInBand ",
    "build": "rm -rf ./dist && vite build",
    "lint": "yarn eslint **/*.ts",
    "link": "yarn link",
    "local-publish": "npm publish --registry http://localhost:4873 --prepatch",
    "prepublish": "yarn lint && yarn test",
    "prepublishOnly": "yarn build",
    "tscNoEmit": "yarn tsc --noEmit"
  },
  "author": "",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/telicent-oss/rdf-libraries.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "openapi-fetch": "0.14.0"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "eslint": "^8.56.0",
    "openapi-typescript": "7.9.1",
    "testcontainers": "^10.14.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsc": "^2.0.4",
    "typescript": "^5.5.4",
    "vite": "^4.5.3",
    "vite-plugin-dts": "^3.6.3",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "engines": {
    "node": ">=20.14.0"
  },
  "resolutions": {
    "rollup": "3.29.5"
  }
}
