{
  "name": "@telicent-oss/sparql-lib",
  "type": "module",
  "version": "0.0.1-TELFE-805.51",
  "private": false,
  "description": "SPARQL functionality",
  "main": "./dist/sparql-lib.cjs.js",
  "module": "./dist/sparql-lib.es.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./dist/sparql-lib.es.js",
      "require": "./dist/sparql-lib.cjs.js"
    },
    "./test-utils": {
      "types": "./test-utils/index.d.ts",
      "import": "./dist/test-utils/sparql-lib-test-utils.es.js",
      "require": "./dist/test-utils/sparql-lib-test-utils.cjs.js"
    }
  },
  "typesVersions": {
    "*": {
      "test-utils": [
        "dist/test-utils/types/index.d.ts"
      ],
      "*": [
        "dist/types/*"
      ]
    }
  },
  "files": [
    "dist/**",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "scripts": {
    "start": "vite",
    "test": "jest --runInBand ",
    "test:container": "DO_TEST_CONTAINERS=1 jest --runInBand ",
    "build": "rm -rf ./dist && yarn build:main",
    "build:main": "vite build -c vite.config.ts",
    "lint": "yarn eslint **/*.ts",
    "link": "yarn link",
    "local-publish": "npm publish --registry http://localhost:4873 --prepatch",
    "prepublish": "yarn lint && echo 'reimplement test'",
    "prepublishOnly": "yarn build",
    "tscNoEmit": "yarn tsc --noEmit"
  },
  "author": "",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/telicent-oss/rdf-libraries.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {},
  "devDependencies": {
    "@telicent-oss/dev-dependencies-lib": "^0.0.1-TELFE-805.1",
    "@types/jest": "^29.4.0",
    "eslint": "^8.56.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsc": "^2.0.4",
    "typescript": "^5.5.4",
    "vite": "^4.5.3",
    "vite-plugin-dts": "^3.6.3",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "peerDependencies": {},
  "engines": {
    "node": ">=20.14.0"
  },
  "resolutions": {
    "rollup": "3.29.5"
  }
}
