  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Telicent-oss/rdf-libraries.git"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Telicent-oss/rdf-libraries/issues"
  },
  "homepage": "https://github.com/Telicent-oss/rdf-libraries#readme",
  "dependencies": {
    "chalk": "^5.3.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@nx/jest": "^17.1.2",
    "@nx/js": "17.1.2",
    "@swc-node/register": "~1.6.7",
    "@swc/core": "~1.3.85",
    "@types/jest": "^29.4.0",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "6.20.0",
    "@typescript-eslint/parser": "6.20.0",
    "babel-jest": "^29.4.1",
    "eslint": "8.56.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.4.1",
    "nx": "17.1.2",
    "prettier": "^2.6.2",
    "testing": "link:@nrwl/nx-plugin/testing",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "nx": {
    "targets": {
      "generate-docs": {
        "outputs": [
          "{projectRoot}/docs"
        ]
      }
