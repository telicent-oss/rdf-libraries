{
  "name": "rdf-libraries",
  "version": "0.0.9",
  "description": "Simple client-side lib for working with ontologies",
  "scripts": {
    "clean": "yarn test --clearCache; yarn clean cache",
    "lint": "lerna run lint",
    "test": "lerna run test",
    "prepublish:all": "lerna run prepublish --stream --no-prefix --concurrency 1",
    "test:stable": "lerna run test --scope=@telicent-oss/catalogservice && yarn test:ci",
    "test:ci": "yarn test --ignore @telicent-oss/catalogservice --ignore @telicent-oss/sparql-lib",
    "build": "lerna run build --stream --no-prefix --concurrency 1",
    "publish:ci": "./scripts/lerna/publish:ci.sh",
    "generate-docs": "lerna run generate-docs",
    "open-docs": "lerna run open-docs",
    "postinstall": "[ \"$LOCAL_MACHINE\" = \"false\" ] && echo 'Skipping tefe hook-postinstall' || tefe hook-postinstall",
    "create-links": "yarn create-links-to-local-packages",
    "local:link": "yarn use-local-packages:link",
    "local-publish": "./scripts/local-publish.mjs",
    "local:unlink": "yarn use-local-packages:unlink",
    "use-links": "yarn use-links-to-local-packages",
    "build:changed": "lerna run build --since HEAD~1 --include-dependents",
    "test:changed": "lerna run test --since HEAD~1 --include-dependents",
    "changed": "lerna changed --loglevel=silent"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Telicent-oss/rdf-libraries.git"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Telicent-oss/rdf-libraries/issues"
  },
  "homepage": "https://github.com/Telicent-oss/rdf-libraries#readme",
  "dependencies": {
    "chalk": "^5.3.0",
    "tslib": "^2.6.3"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@nx/jest": "19.5.4",
    "@swc-node/register": "1.9.2",
    "@swc/core": "1.5.7",
    "@telicent-oss/telicent-frontend-cli": "^1.5.0",
    "@types/jest": "^29.5.12",
    "@types/node": "18.19.9",
    "@typescript-eslint/eslint-plugin": "6.20.0",
    "@typescript-eslint/parser": "6.20.0",
    "babel-jest": "^29.7.0",
    "eslint": "8.56.0",
    "execa": "^9.6.0",
    "glob": "^11.0.3",
    "husky": "^8.0.3",
    "inquirer": "^12.9.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lerna": "8.1.8",
    "prettier": "^2.8.8",
    "ts-jest": "^29.2.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "workspaces": [
    "packages/*"
  ],
  "resolutions": {
    "@telicent-oss/telicent-frontend-cli": "^1.5.0",
    "braces": "^3.0.3",
    "@nrwl/tao": "^19.8.14",
    "@nrwl/devkit": "^19.8.14"
  }
}
