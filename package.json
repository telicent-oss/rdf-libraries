{
  "name": "rdf-libraries",
  "version": "0.0.9",
  "description": "Simple client-side lib for working with ontologies",
  "scripts": {
    "clean": "yarn test --clearCache; yarn clean cache $@",
    "lint": "lerna run lint",
    "test": "lerna run test --ignore=@telicent-oss/catalogservice $@",
    "build": "lerna run build",
    "version:dryrun": "./scripts/version:dryrun.sh",
    "ci:clean": "yarn clean $@",
    "ci:lint": "yarn lint $@",
    "ci:test": "yarn test --ignore=@telicent-oss/catalogservice $@",
    "ci:build": "yarn build $@",
    "ci:publish": "lerna publish --no-private --yes --exact --conventional-commits $@",
    "generate-docs": "lerna run generate-docs",
    "open-docs": "lerna run open-docs",
    "create-links-to-local-packages": "lerna run link",
    "create-links": "yarn create-links-to-local-packages",
    "use-links-to-local-packages": "lerna run use-links-to-local-packages",
    "use-links": "yarn use-links-to-local-packages"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Telicent-oss/rdf-libraries.git"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Telicent-oss/rdf-libraries/issues"
  },
  "homepage": "https://github.com/Telicent-oss/rdf-libraries#readme",
  "dependencies": {
    "chalk": "^5.3.0",
    "tslib": "^2.6.3"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@nx/jest": "19.5.4",
    "@swc-node/register": "1.9.2",
    "@swc/core": "1.5.7",
    "@types/jest": "^29.5.12",
    "@types/node": "18.19.9",
    "@typescript-eslint/eslint-plugin": "6.20.0",
    "@typescript-eslint/parser": "6.20.0",
    "babel-jest": "^29.7.0",
    "eslint": "8.56.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^2.8.8",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.2.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "lerna": "8.1.8"
  },
  "workspaces": [
    "packages/*"
  ],
  "resolutions": {
    "braces": "^3.0.3"
  }
}
